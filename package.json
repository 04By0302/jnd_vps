{
  "name": "lottery-data-backend",
  "version": "1.0.0",
  "description": "Canada 28 Lottery Data Collection System",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env TZ=Asia/Shanghai node dist/app.js",
    "dev": "cross-env TZ=Asia/Shanghai tsx src/app.ts",
    "dev:win": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "import:keno": "cross-env TZ=Asia/Shanghai tsx src/scripts/run-import.ts"
  },
  "keywords": [
    "lottery",
    "data-collection",
    "redis",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/exceljs": "^0.5.3",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.3.2",
    "mysql2": "^3.6.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "rate-limit-redis": "^4.3.1",
    "socks-proxy-agent": "^8.0.5",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.27",
    "cross-env": "^10.1.0",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
